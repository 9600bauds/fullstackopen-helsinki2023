{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": [
      "standard",
      "plugin:react/recommended",
      "plugin:prettier/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "prettier",
      "react"
  ],
  "rules": {
      "prefer-const": "off",
      "linebreak-style": "off",
      "quotes": ["error", "backtick"]
  }
}